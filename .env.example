# WalletConnect Project ID (Get from https://cloud.walletconnect.com/)
VITE_WC_PROJECT_ID=your_32_character_project_id_here

# Helius RPC URL for Solana (Get from https://www.helius.dev/)
# This is REQUIRED for Solana wallets to work properly
VITE_HELIUS_RPC_URL=https://mainnet.helius-rpc.com/?api-key=your_api_key_here

# CoinGecko API (optional, for better rate limits)
VITE_COINGECKO_API_KEY=your_coingecko_api_key_here
